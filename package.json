{
  "name": "@heroku/react-malibu",
  "version": "5.0.2",
  "description": "React components for Heroku's Malibu SVG icon system",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "repository": "https://github.com/heroku/react-malibu",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "scripts": {
    "prepublish": "yarn build",
    "build": "vite build",
    "dev": "vite",
    "start": "yarn build && vite --config demo/vite.config.js",
    "dev:demo": "vite --config demo/vite.config.js",
    "preview": "vite preview",
    "clean": "rm -rf lib dist dist-demo",
    "test": "yarn run lint && yarn run mocha -- --require jsdom-global/register --require @babel/register tests/*_test.js",
    "lint": "yarn run eslint ."
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react-svg-inline": "^2.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": ">=15.0.0 <19.0.0",
    "react-dom": ">=15.0.0 <19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/register": "^7.23.0",
    "@testing-library/react": "^13.4.0",
    "@vitejs/plugin-react": "^5.0.0",
    "chai": "^4.3.10",
    "core-js": "^3.35.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-n": "^16.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-standard": "^5.0.0",
    "fetch-mock": "^9.11.0",
    "jsdom": "^26.1.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.21",
    "mocha": "^11.7.1",
    "node-fetch": "^2.7.0",
    "np": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sinon": "^21.0.0",
    "vite": "^7.1.0",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-lib-inject-css": "^2.2.2"
  },
  "author": "Heroku",
  "homepage": "https://github.com/heroku/react-malibu",
  "license": "SEE LICENSE IN LICENSE.txt",
  "keywords": [
    "react-component"
  ]
}
