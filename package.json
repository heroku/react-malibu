    "react": ">=15.0.0 <19.0.0",
    "react-dom": ">=15.0.0 <19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-node": "^4.2.2",
